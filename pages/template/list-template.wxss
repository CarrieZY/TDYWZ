.tmp-contain{
  display: flex;
  flex-direction: column;
  border-top:1rpx soid #eee;
  border-bottom: 1rpx solid #eee;
  padding: 10rpx 0;
}
.avatar{
  padding: 10rpx;
}
.avatar image{
  width:60rpx;
  height:60rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin:10rpx;
}
.avatar text{
  font-size: 32rpx;
}
.title{
  margin-left: 10rpx;
  font-size: 36rpx;
  margin:10rpx;
}
.content-img{
  width:100%;
  height:460rpx;
}
.content{
  font-size: 32rpx;
  text-indent: 32rpx;
}
.collection image{
  width:32rpx;
  height:32rpx;
  vertical-align: middle;
}
.collection text{
  font-size: 28rpx;
  margin-left:10rpx;
}